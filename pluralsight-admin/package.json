  "description": "A PluralSight Project for restacking to react",
  "scripts": {
    "app:init": "npm run app:clean && npm install && npm start",
    "app:clean": "rm -rf ./node_modules/ || true && rm -rf ./build/ || true",
    "start": "npm run build:server && node ./build/index.js",
    "test": "nyc ./node_modules/mocha/bin/mocha --require ./mocha.config.js \"./client/tests/**/*.test.js*(x)\"",
    "build:server": "rm -rf ./build && babel ./server --out-dir ./build"
  },
